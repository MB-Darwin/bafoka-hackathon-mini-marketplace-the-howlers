#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Red Emperor/work/mini-marketplace/.pnpm-store/acorn@8.15.0/node_modules/acorn/bin/node_modules:/proc/cygdrive/c/Users/Red Emperor/work/mini-marketplace/.pnpm-store/acorn@8.15.0/node_modules/acorn/node_modules:/proc/cygdrive/c/Users/Red Emperor/work/mini-marketplace/.pnpm-store/acorn@8.15.0/node_modules:/proc/cygdrive/c/Users/Red Emperor/work/mini-marketplace/.pnpm-store/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Red Emperor/work/mini-marketplace/.pnpm-store/acorn@8.15.0/node_modules/acorn/bin/node_modules:/proc/cygdrive/c/Users/Red Emperor/work/mini-marketplace/.pnpm-store/acorn@8.15.0/node_modules/acorn/node_modules:/proc/cygdrive/c/Users/Red Emperor/work/mini-marketplace/.pnpm-store/acorn@8.15.0/node_modules:/proc/cygdrive/c/Users/Red Emperor/work/mini-marketplace/.pnpm-store/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../.pnpm-store/acorn@8.15.0/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../.pnpm-store/acorn@8.15.0/node_modules/acorn/bin/acorn" "$@"
fi
